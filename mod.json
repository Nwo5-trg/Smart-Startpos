{
	"geode": "4.2.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074"
	},
	"id": "nwo5.smart_startpos",
	"name": "Smart Startpos",
	"version": "v1.1.1",
	"developer": "Nwo5",
	"description": "Simple smart startpos mod.",
	"tags": ["editor", "enhancement", "offline"],
	"links": {
		"source": "https://github.com/Nwo5-trg/Smart-Startpos"
	},
	"dependencies": [
        {
            "id": "blueblock6.cleanstartpos",
            "version": ">=v1.3.1",
            "importance": "recommended"
        }
    ],
	"settings": {
		"general-title": {
			"type": "title",
			"name": "General"
		},
		"mode": {
			"type": "string",
			"name": "Mode",
			"description": "On Place sets startpos settings when it's placed. On Playtest sets the settings of *all* startpos objects on playtest.",
			"default": "On Place",
			"one-of": ["On Place", "On Playtest"]
		},
		"toggles-title": {
			"type": "title",
			"name": "Toggles"
		},
		"speed-toggle": {
			"type": "bool",
			"name": "Set Speed",
			"default": true
		},
		"gamemode-toggle": {
			"type": "bool",
			"name": "Set Gamemode",
			"default": true
		},
		"gravity-toggle": {
			"type": "bool",
			"name": "Set Gravity",
			"description": "Determines gravity strictly based off portals and orbs.",
			"default": false
		},
		"mirror-toggle": {
			"type": "bool",
			"name": "Set Mirror",
			"default": true
		},
		"mini-toggle": {
			"type": "bool",
			"name": "Set Mini",
			"default": true
		},
		"dual-toggle": {
			"type": "bool",
			"name": "Set Dual",
			"default": true
		}
	}
}